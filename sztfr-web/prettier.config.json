{
  "tabWidth": 2,
  "useTabs": false,
  "singleQuote": false,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "semicolons": true,
  "jsxBracketSameLine": true,
  "arrowParens": "always",
  "printWidth": 80,
  "endOfLine": "auto",
  "overrides": [
    {
      "files": ["*.scss", "*.css"],
      "options": {
        "singleQuote": false
      }
    }
  ]
}
